% Helper commands >>>1

\newcommand\FirstWordUpper[1]{\@firstwordupper#1 \@nil}% >>>2
\newcommand\@firstwordupper{}%
\def\@firstwordupper#1 #2\@nil{\MakeUppercase{#1} #2\unskip}% <<<2

\newcommand\FirstWordSC[1]{\@firstwordsc#1 \@nil}% >>>2
