% Misc >>>2

\xpatchbibmacro{name:andothers}{%% >>>3
  \bibstring{andothers}%
}{%
  \mkbibemph{\bibstring{andothers}}%
}{}{}%% <<<3

\renewbibmacro*{in:}{%% >>>3
  \mkbibemph{\bibstring{in}\intitlepunct}
}%% <<<3

\renewbibmacro*{volume+number+eid}{%% >>>3
  \printfield{volume}%
  \setunit*{\addcomma\addspace}%
  \printfield{number}%
  \setunit{\addcomma\addspace}%
  \printfield{eid}%
